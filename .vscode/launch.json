{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Langgraph Dev",
            "type": "node",
            "request": "launch",
            "runtimeExecutable": "bash",
            "runtimeArgs": [
                "-c",
                "PYTHONPATH=$PYTHONPATH:\"src\" langgraph dev"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Debug graph_example.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/examples/graph_example.py",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            }
        },
        {
            "name": "Debug dataset_manager_example.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/examples/dataset_manager_example.py",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            }
        },
        {
            "name": "Debug API",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/run.py",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            },
            "justMyCode": false
        }
    ]
}